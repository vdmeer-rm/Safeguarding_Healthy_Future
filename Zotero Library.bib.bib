  title = {Open {{Science}} in {{Practice}}: {{Researcher Perspectives}} and {{Participation}}},
  shorttitle = {Open {{Science}} in {{Practice}}},
  author = {Whyte, Angus and Pryor, Graham},
  date = {2011-03-11},
  journaltitle = {International Journal of Digital Curation},
  volume = {6},
